<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- disabled -->
    <item android:state_enabled="false" android:color="@color/com_adobe_image_color_dark_gray1_disabled" />

    <!-- selected -->
    <item android:state_selected="true" android:color="@color/com_adobe_image_color_blue_dark" />
    <item android:state_activated="true" android:color="@color/com_adobe_image_color_blue_dark" />

    <!-- pressed -->
    <item android:state_pressed="true" android:color="@color/com_adobe_image_color_blue_dark" />

    <!-- default -->
    <item android:state_window_focused="false" android:color="@color/com_adobe_image_color_blue" />
    <item android:color="@color/com_adobe_image_color_blue" />

</selector>