<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- default state -->
    <item
        android:state_enabled="true"
        android:drawable="@drawable/com_adobe_image_flat_button_blue_transparent_shape_enabled" />

    <!-- pressed state -->
    <item
        android:state_pressed="true"
        android:drawable="@drawable/com_adobe_image_flat_button_blue_transparent_shape_pressed"/>

    <!-- disabled state -->
    <item
        android:state_enabled="false"
        android:drawable="@drawable/com_adobe_image_flat_button_blue_transparent_shape_disabled"/>
</selector>