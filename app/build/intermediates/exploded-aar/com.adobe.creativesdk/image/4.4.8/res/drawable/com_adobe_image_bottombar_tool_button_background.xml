<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- pressed -->
    <item android:state_checked="false">
        <shape android:shape="oval">
            <stroke android:width="3dip" android:color="@color/com_adobe_image_color_light_gray_semi_transparent1" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>

    <!-- pressed -->
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <stroke android:width="3dip" android:color="@color/com_adobe_image_color_light_gray_semi_transparent1" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>

    <!-- checked -->
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/com_adobe_image_color_light_gray_semi_transparent1" />
        </shape>
    </item>

    <!-- selected -->
    <item android:state_selected="true">
        <shape android:shape="oval">
            <solid android:color="@color/com_adobe_image_color_light_gray_semi_transparent1" />
        </shape>
    </item>

    <!-- diabled -->
    <item android:state_enabled="false">
        <shape android:shape="oval">
            <stroke android:width="3dip" android:color="@color/com_adobe_image_color_light_gray_semi_transparent0" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
</selector>
